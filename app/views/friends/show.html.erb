<div class="jumbotron">
  <h1 class="display-4">
  <%= @friend.first_name %>
  <%= @friend.last_name %>
  </h1>
  <p class="lead"></p>
  <hr class="my-4">
  <p>
  <strong>Email:</strong>
  <%= @friend.email %> 
  </p> 
  <p>
  <strong>Phone:</strong>
  <%= @friend.phone %>
  </p>
  <p>
  <strong>Twitter:</strong>
  <%= @friend.twitter %>
  </p>
  <%= link_to 'Edit', edit_friend_path(@friend), class:'btn btn-outline-secondary' %>
  <%= link_to 'Back', friends_path, class:'btn btn-outline-dark'  %>
  <br/>
  <br/>
</div>


